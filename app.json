{
    "name": "Piggy Bank",
    "description": "Starter application for using the REST API of ePages BEYOND.",
    "website": "https://signup.beyondshop.cloud/",
    "repository": "https://github.com/ePages-de/piggy-bank",
    "logo": "https://github.com/ePages-de/piggy-bank/blob/master/src/assets/piggy-bank.png?raw=true",
    "keywords": [
        "vuejs",
        "beyond",
        "rest-api"
    ],
    "buildpacks": [
        {
            "url": "heroku/nodejs"
        }
    ],
    "env": {
        "NPM_CONFIG_PRODUCTION": {
            "value": "false",
            "description": "Only install `dependencies`, but not `devDependencies`"
        },
        "YARN_PRODUCTION": {
            "value": "false",
            "description": "Only install `dependencies`, but not `devDependencies`"
        }
    }
}
